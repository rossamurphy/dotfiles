{
  "jedi.enable": true,
  "jedi.startupMessage": false,
  "jedi.executable.command": "jedi-language-server",
  "suggest.echodocSupport": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx"
  ],
  "languageserver": {
    "python": {
      "command": "jedi-language-server",
      "filetypes": ["python"]
    }
  }
}
